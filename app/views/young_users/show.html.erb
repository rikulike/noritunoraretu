<h4>ユーザーネーム</h4>
<%= @young_user.name %>
<h4>紹介文</h4>
<%= @young_user.introduction %>
<div>
  <% if @young_user==current_young_user %>
    <%= link_to "プロフィール編集", edit_young_user_path(current_young_user.id) %>
  <% end %>
</div>


<% @wisdom_posts.each do |wisdom_post| %>
<div>
  <p>タイトル</p>
   <%= link_to wisdom_post_path(wisdom_post.id) do %>
     <%= wisdom_post.title %>
   <% end %>
  <p>本文</p>
  <%= wisdom_post.body %>
  <p>投稿日時</p>
  <%= wisdom_post.created_at.strftime('%y/%m/%d') %>
</div>
<% end %>

